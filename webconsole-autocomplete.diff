-  if (typeof obj === "undefined" || obj === null) {
+  // If obj is undefined or null (which is what "== null" does),
+  // then there is no chance to run completion on it. Exit here.
+  if (obj == null) {
     return null;
   }
 
@@ -929,8 +929,25 @@ function JSPropertyProvider(aScope, aInp
     return null;
   }
 
+  // Array of strings of all properties on this object.
+  function getScope(obj) {
+    let names = {};
+    let ownNames = Object.getOwnPropertyNames(obj);
+    for (let i = 0; i < ownNames.length; i++) {
+      names[ownNames[i]] = 1;
+    }
+    obj = Object.getPrototypeOf(obj);
+    if (obj !== null) {
+      let parentScope = getScope(obj);
+      for (let name in parentScope) {
+        names[name] = 1;
+      }
+    }
+    return names;
+  }
+
   let matches = [];
-  for (let prop in obj) {
+  for (let prop in getScope(obj)) {
     if (prop.indexOf(matchProp) == 0) {
       matches.push(prop);
     }
